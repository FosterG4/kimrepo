wget --load-cookies /tmp/cookies.txt "https://docs.google.com/uc?export=download&confirm=$(wget --quiet --save-cookies /tmp/cookies.txt --keep-session-cookies --no-check-certificate 'https://docs.google.com/uc?export=download&id=1_kAo5ZR9I-MLTpw8IpBIGmSVFZqI3Cre' -O- | sed -rn 's/.*confirm=([0-9A-Za-z_]+).*/\1\n/p')&id=1_kAo5ZR9I-MLTpw8IpBIGmSVFZqI3Cre" -O win2012.gz && rm -rf /tmp/cookies.txt

wget -O- https://url/win2012.gz | gunzip | dd of=/dev/vda

